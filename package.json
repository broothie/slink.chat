{
  "name": "slink.chat",
  "version": "1.0.0",
  "main": "www/js/index.js",
  "author": "broothie <adbooth8@gmail.com>",
  "license": "MIT",
  "scripts": {
    "scripts": "jq .scripts package.json",
    "server": "go run cmd/server/main.go",
    "server:watch": "gin -i -d cmd/server --notifications",
    "jobs": "go run cmd/job/main.go",
    "jobs:watch": "gin -i -p 3010 -a 3011 -d cmd/job --notifications",
    "init": "go run cmd/init/main.go",
    "js": "go run cmd/bundle/main.go --production",
    "js:watch": "go run cmd/bundle/main.go --watch",
    "css": "tailwindcss -c config/tailwind.config.js --postcss config/postcss.config.js -i www/css/style.css -o static/style.css --minify",
    "css:watch": "tailwindcss -c config/tailwind.config.js --postcss config/postcss.config.js -i www/css/style.css -o static/style.css --minify --watch",
    "issues": "gh issue list",
    "issues:create": "gh issue create --body '' --title"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.4",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/luxon": "^3.3.4",
    "autoprefixer": "^10.4.8",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.3",
    "postcss": "^8.4.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "socket.io-client": "^4.5.1",
    "tailwindcss": "^3.1.8"
  }
}
